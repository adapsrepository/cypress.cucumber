{
  "name": "orangehrm_ci_bdd_reports",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:headless": "cypress run --spec 'cypress/integration/features/**.feature'",
    "cy:dashboard": "cypress run --record --key 3551a3cd-a91e-4c08-8fac-89440bbc7b04 --spec 'cypress/integration/features/**.feature'",
    "cy:run": "cypress run --env allure=true,allureResultsPath=allure-results",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "(if exist allure-report (rmdir /S /Q allure-report)) && (if exist allure-results (rmdir /S /Q allure-results))",
    "pretest": "npm run allure:clear",
    "posttest": "npm run allure:report",
    "test": "npm run cy:run || npm run posttest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.6.0",
    "@shelex/cypress-allure-plugin": "^2.19.5",
    "allure-commandline": "^2.13.8",
    "as-a": "^1.4.1",
    "cypress": "^8.3.0",
    "cypress-cucumber-preprocessor": "^4.0.3",
    "cypress-localstorage-commands": "^1.4.0",
    "cypress-react-selector": "2.2.0",
    "cypress-social-logins": "^1.9.2",
    "cypress-xpath": "^1.6.2",
    "moment": "^2.29.1",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "ts-loader": "^8.1.0",
    "typescript": "^3.9.9",
    "webpack": "^5.28.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/integration/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {}
}